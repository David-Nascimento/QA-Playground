{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".",
  "framework": null,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/js/(.*\\.js)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        }
      ]
    },
    {
      "source": "/css/(.*\\.css)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css; charset=utf-8"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/pages/junior$",
      "destination": "/pages/junior/index.html"
    },
    {
      "source": "/pages/pleno$",
      "destination": "/pages/pleno/index.html"
    },
    {
      "source": "/pages/senior$",
      "destination": "/pages/senior/index.html"
    },
    {
      "source": "/pages/fundamentos$",
      "destination": "/pages/fundamentos/index.html"
    },
    {
      "source": "/pages/mobile$",
      "destination": "/pages/mobile/index.html"
    },
    {
      "source": "/pages/desafio$",
      "destination": "/pages/desafio/index.html"
    },
    {
      "source": "/pages/junior/$",
      "destination": "/pages/junior/index.html"
    },
    {
      "source": "/pages/pleno/$",
      "destination": "/pages/pleno/index.html"
    },
    {
      "source": "/pages/senior/$",
      "destination": "/pages/senior/index.html"
    },
    {
      "source": "/pages/fundamentos/$",
      "destination": "/pages/fundamentos/index.html"
    },
    {
      "source": "/pages/mobile/$",
      "destination": "/pages/mobile/index.html"
    },
    {
      "source": "/pages/desafio/$",
      "destination": "/pages/desafio/index.html"
    },
    {
      "source": "/pages/junior/formularios$",
      "destination": "/pages/junior/formularios/index.html"
    },
    {
      "source": "/pages/junior/formularios/$",
      "destination": "/pages/junior/formularios/index.html"
    },
    {
      "source": "/pages/junior/interacoes$",
      "destination": "/pages/junior/interacoes/index.html"
    },
    {
      "source": "/pages/junior/interacoes/$",
      "destination": "/pages/junior/interacoes/index.html"
    },
    {
      "source": "/pages/junior/tabelas$",
      "destination": "/pages/junior/tabelas/index.html"
    },
    {
      "source": "/pages/junior/tabelas/$",
      "destination": "/pages/junior/tabelas/index.html"
    },
    {
      "source": "/pages/pleno/arquivos$",
      "destination": "/pages/pleno/arquivos/index.html"
    },
    {
      "source": "/pages/pleno/arquivos/$",
      "destination": "/pages/pleno/arquivos/index.html"
    },
    {
      "source": "/pages/pleno/interacoes$",
      "destination": "/pages/pleno/interacoes/index.html"
    },
    {
